#include <stdio.h>

void calcularTabuada(int numero) {
    printf("Tabuada de %d:\n", numero);
    for (int i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", numero, i, numero * i);
    }
}

int main() {
    int numero;

    printf("Digite um nÃºmero para calcular a tabuada: ");
    scanf("%d", &numero);

    calcularTabuada(numero);

    return 0;
}
